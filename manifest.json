{
    "manifest_version": 2,

    "name": "KnowIt!",
    "description": "We need a slogan",
    "version": "0.0.1",

    "background": {
        "scripts": ["background.js"]
    },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content_script.js"]
        }
    ],

    "permissions": [
        "activeTab",
        "contextMenus"
    ]
}
